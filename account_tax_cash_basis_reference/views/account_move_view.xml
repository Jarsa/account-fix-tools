<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record model='ir.ui.view' id='account_move_cash_basis_form'>
            <field name='name'>account.move.cash.basis.form</field>
            <field name='model'>account.move</field>
            <field name='inherit_id' ref='account.view_move_form'/>
            <field name='arch' type='xml'>
                <div class="oe_button_box" position="inside">
                    <button class="oe_stat_button" icon="fa-files-o" name="account_move" type="object">
                        <field name="nbr_moves" widget="statinfo"/>
                    </button>
                </div>
            </field>
        </record>
    </data>
</odoo>
